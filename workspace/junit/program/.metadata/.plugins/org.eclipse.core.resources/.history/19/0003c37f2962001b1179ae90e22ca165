package com.cybage.junit_demo;

import static org.junit.jupiter.api.Assertions.assertEquals;

import org.junit.Test;
import org.junit.jupiter.api.Tag;

public class AppTest 
{
    @Tag("DEV")
    @Test
    void test1() {
        assertEquals(1, 1);
    }

 
    @Tag("PROD")
    @Test
    void test2() {
        assertEquals(10, 10);
    }
}