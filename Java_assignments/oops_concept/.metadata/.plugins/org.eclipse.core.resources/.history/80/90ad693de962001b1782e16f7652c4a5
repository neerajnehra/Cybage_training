package com.cybage;

public class EmployeeDetails {
	String name, type;
	int salary;
	
	public EmployeeDetails(String name, String type, int salary) {
		super();
		this.name = name;
		this.type = type;
		this.salary = salary;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public int getSalary() {
		return salary;
	}

	public void setSalary(int salary) {
		this.salary = salary;
	}

	@Override
	public String toString() {
		return "EmployeeDetails \nName=" + name + "\nType=" + type + "\nSalary=" + salary + "]";
	}
	
	
}
