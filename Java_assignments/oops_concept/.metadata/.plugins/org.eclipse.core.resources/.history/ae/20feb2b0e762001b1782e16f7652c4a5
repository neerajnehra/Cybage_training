package com.cybage;

public class Current extends BankAccount{
	double balance, rateOfInterest = 0;
	
	public Current(String name, String bankName, double balance, double rateOfInterest) {
		super(name, bankName);
		this.balance = balance;
		this.rateOfInterest = rateOfInterest;
	}




	double showYearlyDeduction() {
		return balance;
	}

}
