package com.cybage;

public class SalesManager {
	int com;

	public SalesManager(int com) {
		super();
		this.com = com;
	}

	public int getCom() {
		return com;
	}

	public void setCom(int com) {
		this.com = com;
	}

	@Override
	public String toString() {
		return "SalesManager [compensation=" + com + "]";
	}
	
}
