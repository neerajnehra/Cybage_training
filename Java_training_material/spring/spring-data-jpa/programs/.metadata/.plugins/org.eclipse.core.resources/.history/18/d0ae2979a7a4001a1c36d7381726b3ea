package com.cybage;

import javax.persistence.Entity;

@Entity
public class Regular extends Employee{
	private int salary;

	public Regular() {
		super();
	}

	public Regular(int id, String name ) {
		super(id, name);
	}

	public Regular(int id, String name, int salary) {
		super(id, name);
		this.salary = salary;
	}
	

}
